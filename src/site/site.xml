<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/DECORATION/1.4.0"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.4.0 http://maven.apache.org/xsd/decoration-1.4.0.xsd"
         name="EEUtils">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.1</version>
    </skin>
    <bannerLeft>
        <name>${project.name}</name>
        <href>/</href>
    </bannerLeft>
    <body>
        <links>
            <item name="Source" href="${project.scm.url}" target="_blank"/>
            <item name="Continuous Integration" href="${project.ciManagement.url}" target="_blank"/>
        </links>
        <menu name="${project.name}">
            <item name="Homepage" href="/"/>
            <item name="Source" href="${project.scm.url}" target="_blank"/>
            <item name="Continuous Integration" href="${project.ciManagement.url}" target="_blank"/>
            <item name="Maven Repo" href="http://public.repo.larry1123.net/" target="_blank"/>
        </menu>
        <menu name="API Documentation">
            <item name="JavaDocs" href="apidocs/index.html"/>
        </menu>
        <menu ref="reports"/>
    </body>
    <custom>
        <fluidoSkin>
            <googleSearch>
                <sitesearch/>
            </googleSearch>
        </fluidoSkin>
    </custom>
</project>
